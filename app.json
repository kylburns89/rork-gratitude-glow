{
  "expo": {
    "name": "Gratitude Glow",
    "slug": "gratitude-glow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.recursiverabbit.gratitudeglow",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.recursiverabbit.gratitudeglow",
      "permissions": [
        "android.permission.VIBRATE",
        "android.permission.HIGH_SAMPLING_RATE_SENSORS",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "com.android.vending.BILLING"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          },
          "android": {
            "minSdkVersion": 23,
            "targetSdkVersion": 34
          }
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ]
    ],
    "extra": {
      "revenuecat": {
        "iosApiKey": "appl_oFKdOlXHWGTFTyVRpczUIKmMGSq",
        "androidApiKey": "YOUR_REVENUECAT_ANDROID_API_KEY",
        "entitlement": "premium",
        "offering": "Premium"
      },
      "eas": {
        "projectId": "1aba70a0-533c-437a-a8b4-2697a89e9a11"
      },
      "router": {
        "origin": "https://rork.com/"
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "owner": "autopilot715"
  }
}
